import java.util.*;
class Main {
    public static void main(String[] args) {
        int v=-1;
       int arr[]={1,2,3,2,4,1};
       for(int i=0;i<arr.length;i++){
           if(arr[i]!=-1){
               for(int j=i+1;j<arr.length;j++){
                   if(arr[i]==arr[j]){
                       arr[j]=v;
                   }
               }
               System.out.print(arr[i]);
           }
       }
    }
}


