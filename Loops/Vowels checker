import java.util.Scanner;

public class vowels {

public static void main(String[] args) {

String n="joy";

int count=0,cou=0;

for(int i=0;i<n.length();i++)

{

if(n.charAt(i)=='a' ||n

.charAt(i)=='e' ||n

.charAt(i)=='i' ||n

.charAt(i)=='o' ||n

.charAt(i)=='u'){

System.out.println("vowels are:"+n.charAt(i));
}
else {
System.out.println("consonents are:"+n.charAt(i));
}
}
}
 }
