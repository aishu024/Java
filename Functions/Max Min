import java.util.Scanner;

public class num{

public static void main(String[] args) {

min(10,3,8);
max(10,3,8);
}
static void max(int a, int b,int c){

int d=a>b?a:b;

int larg=d>c?d:c;

System.out.println("largest value is: "+larg);
}
static void min(int a, int b, int c) {

int d=a<b?a:b;

int min=d<c?d:c;

System.out.println("minimum is:" +min);

}}
