package datastructure;
import java.util.Arrays;

public class bubble {
 public static void main(String args[]) {
	 int[] a= {3,1,6,0,2,5,4 };
	 for(int i=0;i<a.length-1;i++) {
		 for(int j=0;j<a.length-1;j++) {
			 if(a[j]>a[j+1]) {
				 int temp=a[j];
				 a[j]=a[j+1];
				 a[j+1]=temp;
			 }
		 }
		 }
		 System.out.println(Arrays.toString(a));
	 }

}
